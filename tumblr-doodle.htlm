{block:PermalinkPage}
    <div class="uk-grid uk-grid-width-1-1">
        {block:Posts}
        <div class=".uk-article">
            {block:PostTitle}
            <h2 class="uk-article-title">{PostTitle}</h2>
            {/block:PostTitle}
            {block:Caption}
            <p class="uk-article-lead">{Caption}</p>
            {/block:Caption}
            <p class="uk-article-meta">Posted {TimeAgo} by {PostAuthorName}</p>
            <hr class="uk-article-divider">            
                        
            
            
            

                {block:Photo}
                    <!--       
                         ____  __  ______  __________ 
                        / __ \/ / / / __ \/_  __/ __ \
