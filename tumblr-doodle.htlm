                  |___/___/_____/_____/\____/  
                                                                       
                -->   
 

                 <div class="mediavideo">{VideoEmbed-700}</div>

                {/block:Video}  
        
              
            </div>
            {/block:Posts}
        </div>
{/block:PermalinkPage}




{block:IndexPage} 
    
    <div class="uk-grid uk-grid-width-small-1-1 uk-grid-width-medium-1-2 uk-grid-width-large-1-3" data-uk-grid="{gutter: 25}">
      {block:Posts}
        {block:Photo}
            <!--       
                 ____  __  ______  __________ 
                / __ \/ / / / __ \/_  __/ __ \
               / /_/ / /_/ / / / / / / / / / /
              / ____/ __  / /_/ / / / / /_/ / 
             /_/   /_/ /_/\____/ /_/  \____/  
                                              
            -->    
        <div>
            <div class="uk-thumbnail">
                <img src="{PhotoURL-500}" width="{PhotoWidth-500}"height="{PhotoHeight-500}" />
                {block:Caption}
                <div class="uk-thumbnail-caption">{Caption}</div>
                {/block:Caption}
                <a class=" uk-float-right uk-text-muted uk-text-small" href="{Permalink}"> {TimeAgo}</a>
            </div>
        </div>
        {/block:Photo}

        {block:Photoset}
            <!--       
                ____  __  ______  __________  _____ ____________
               / __ \/ / / / __ \/_  __/ __ \/ ___// ____/_  __/
              / /_/ / /_/ / / / / / / / / / /\__ \/ __/   / /   
             / ____/ __  / /_/ / / / / /_/ /___/ / /___  / /    
            /_/   /_/ /_/\____/ /_/  \____//____/_____/ /_/                                 
            -->     
        <div>
          <div class="uk-thumbnail">
                {block:Photos}
                <img src="{PhotoURL-500}" width="{PhotoWidth-500}"height="{PhotoHeight-500}" />
                {/block:Photos}
                {block:Caption}
                <div class="uk-thumbnail-caption">{Caption}</div>
                {/block:Caption}
                <a class=" uk-float-right uk-text-muted uk-text-small" href="{Permalink}"> {TimeAgo}</a>
          </div>
        </div>
        {/block:Photoset}

                                                 
        {block:Video}
        <!--       
           _    __________  __________ 
          | |  / /  _/ __ \/ ____/ __ \
          | | / // // / / / __/ / / / /
          | |/ // // /_/ / /___/ /_/ / 
          |___/___/_____/_____/\____/  
                                                               
        -->   
            <div>
                <div class="uk-thumbnail  uk-thumbnail-large">
                    <div class="mediavideo">{VideoEmbed-700}</div>
                    {block:Caption}
                    <div class="uk-thumbnail-caption uk-clearfix">{Caption}</div>
                    {/block:Caption}
                    <a class=" uk-float-right uk-text-muted uk-text-small" href="{Permalink}"> {TimeAgo}</a>
                </div>
            </div>
        {/block:Video}  

      {/block:Posts}
    </div>

